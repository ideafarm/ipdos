        timeStale1P = timeN1 ;                                                  \
        timeStale2P = timeN2 ;                                                  \
        (etherP).osTimeNowF( tinP , timeN1 , timeN2 ) ;                         \
        ZE( sCountT , timeN2 ) ;                                              \
        ZE( countT  , timeN1 ) ;                                              \
